{
  "manifest_version": 3,
  "name": "New Tab Home",
  "version": "1.6.1",
  "description": "There's no place like home.",
  "author": "Roly",
  "homepage_url": "https://www.github.com/ylor/newtab-home",
  "icons": {
    "48": "img/icon.svg",
    "96": "img/icon.svg"
  },
  "permissions": [
    "browserSettings",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_ui": {
    "page": "options.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "newtab@home"
    }
  }
}